<div class="">

  foo

  {% for block in section.blocks %}

    {% if block.type == 'something' %}

      {{ block.settings }}

    {% elsif block.type == "xxx" %}

      {{ block.settings.foooo }}

      {% render 'foo'
       , param: 1
       , param: 2
       , param: 3 %}

    {% endif %}


  {% endfor %}

</div>


{% schema %}
  {
    "name": "dsss",
    "settings": [
      {
        "type": "page",
        "id": "",
        "default": 0
      }
    ],
    "blocks": [
      {
        "name": "Foo",
        "type": "something",
        "settings": [
          {
            "type": "text",
            "id": "xxxx",
            "label": "foo"
          }
        ]
      },
      {
        "name": "example",
        "type": "xxx",
        "settings": [
          {
            "type": "article",
            "id": "drive",
            "label": "car",
            "default": 0
          },
          {
            "type": "text",
            "id": "foooo",
            "label": "Some Foooo",
            "default": 0
          },
          {
            "default": 0,
            "type": "blog",
            "id": "foo",
            "label": "bar"
          }
        ]
      }
    ]
  }
{% endschema %}
