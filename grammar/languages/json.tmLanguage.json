{
  "name": "Liquid JSON",
  "scopeName": "source.json.liquid",
  "fileTypes": ["liquid"],
  "injectionSelector": "L:source.json",
  "injections": {
    "L:string - (string.quoted.*.json)": {
      "patterns": [
        { "include": "source.liquid#tags" },
        { "include": "source.liquid#output" }
      ]
    }
  },
  "patterns": [
    { "include": "source.liquid#comments" },
    { "include": "source.liquid#tags" },
    { "include": "source.liquid#output" },
    { "include": "source.json" }
  ]
}
