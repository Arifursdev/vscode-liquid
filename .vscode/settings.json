{
  "cSpell.words": [
    "Allman",
    "esbenp",
    "Mansedan",
    "packagejson",
    "TLDR",
    "VSIX"
  ],
  "files.associations": {
    "*.json.liquid": "json",
    "*.yaml.liquid": "yaml",
    "*.md.liquid": "markdown",
    "*.js.liquid": "liquid-javascript",
    "*.css.liquid": "liquid-css",
    "*.scss.liquid": "liquid-scss"
  },
  "json.schemas": [
    {
      "fileMatch": [
        "package.json"
      ],
      "url": "https://unpkg.com/tsup/schema.json"
    }
  ],
  "liquid.format.rules": {
    "json": {}
  }
}